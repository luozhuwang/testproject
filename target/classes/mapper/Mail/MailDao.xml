<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sy.dao.MailDao">
	
	<insert id="insertMail">	    
	    insert into mail_record (subject,to_mail,cc_mail,content)
	    values
	    (#{subject},#{to_mail},#{cc_mail},#{content})	
	</insert>
	
</mapper>	